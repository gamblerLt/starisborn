+
+        Scanner scanner = new Scanner(System.in);
+        System.out.println("Įveskite x reikšmę: ");
+        int x = scanner.nextInt();
+        if(x > 0){
+            int result1 = 2 * x + 8;
+            System.out.println(result1);
